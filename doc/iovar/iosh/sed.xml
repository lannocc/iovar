          xmlns:db="http://docbook.org/ns/docbook">
  <refnamediv>
    <refname>sed</refname>

    <refpurpose>Stream-EDit: filter input stream</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis label="/bin/sed" role="iosh">
      <command role="iosh">sed</command>

      <arg>FIXME</arg>
    </cmdsynopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Examples</title>

    <para>Here are some examples.</para>

    <example>
      <title>Output a text file, replacing occurrences of the word "foo" with
      "bar"</title>

      <programlisting language="shell">cat /foobar.txt | sed ?filter='s/foo/bar/g'</programlisting>
    </example>
  </refsection>

  <refsection>
    <title>Source</title>

    <programlisting language="java"
