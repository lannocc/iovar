  <refnamediv>
    <refname>uptime</refname>

    <refpurpose>Display the IOVAR platform uptime</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis label="/bin/uptime" role="iosh">
      <command role="iosh">uptime</command>
    </cmdsynopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Examples</title>

    <para>Here are some examples.</para>

    <example>
      <title>How long has it been running?</title>

      <programlisting language="shell">uptime</programlisting>

      <screen>up 0 days 5 hours 4 minutes</screen>
    </example>
  </refsection>

